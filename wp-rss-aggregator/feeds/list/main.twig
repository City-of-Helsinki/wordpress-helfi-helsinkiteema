{{ helsinki_wp_do_action( 'helsinki_wpra_feed_posts_list_before', items, options ) }}

<div class="wp-rss-aggregator feed-posts {{ options.html_classes }} {{ options.className }}" data-page="{{ pagination.page }}">

  {{ helsinki_wp_do_action( 'helsinki_wpra_feed_posts_list_top', items, options ) }}

  {{ include(self.dir ~ 'feed-list.twig') }}
  {% if options.pagination_enabled and pagination.num_pages > 1 %}
      {{ include(self.dir ~ options.pagination_type ~ '-pagination.twig') }}
  {% endif %}

  {{ helsinki_wp_do_action( 'helsinki_wpra_feed_posts_list_bottom', items, options ) }}

</div>

{{ helsinki_wp_do_action( 'helsinki_wpra_feed_posts_list_after', items, options ) }}
